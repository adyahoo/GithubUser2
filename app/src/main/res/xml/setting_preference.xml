<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:iconSpaceReserved="false">

    <SwitchPreference
        android:icon="@drawable/ic_baseline_notifications_24"
        android:key="@string/key_reminder"
        android:title="@string/title_reminder"
        android:summaryOff="@string/switch_off"
        android:summaryOn="@string/switch_on"/>

    <Preference
        android:key="@string/key_language"
        android:title="@string/select_language"
        android:icon="@drawable/ic_baseline_language_24"
        android:enabled="true"
        android:selectable="true"/>

</PreferenceScreen>